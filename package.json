{
  "name": "@hello.nrfcloud.com/proto",
  "version": "0.0.0-development",
  "description": "Documents the communication protocol between the hello.nrfcloud.com backend and the web application",
  "type": "module",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./export.js",
      "node": "./export.js"
    },
    "./fingerprint": {
      "import": "./dist/fingerprint/export.js",
      "node": "./dist/fingerprint/export.js",
      "types": "./fingerprint/index.d.ts"
    },
    "./hello": {
      "import": "./dist/hello/export.js",
      "node": "./dist/hello/export.js",
      "types": "./hello/index.d.ts"
    },
    "./nrfCloud": {
      "import": "./dist/nrfCloud/export.js",
      "node": "./dist/nrfCloud/export.js",
      "types": "./hello/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hello-nrfcloud/proto.git"
  },
  "bugs": {
    "url": "https://github.com/hello-nrfcloud/proto/issues"
  },
  "homepage": "https://hello.nrfcloud.com",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "hello-nrfcloud"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "prepublishOnly": "./compile.sh"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.50",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "15.0.12",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.67",
    "@swc/jest": "0.2.26",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.3",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "chalk": "5.3.0",
    "eslint": "8.44.0",
    "glob": "10.3.1",
    "husky": "8.0.3",
    "jest": "29.6.0",
    "lint-staged": "13.2.3",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "3.2.2",
    "tsx": "3.12.7",
    "typescript": "5.1.6"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga|v[0-9].[0-9].x|*_*|*/*)",
        "prerelease": true
      }
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "LICENSE",
    "README.md",
    "example.ts",
    "export.js",
    "index.d.ts",
    "dist",
    "nrfCloud",
    "hello",
    "fingerprint",
    "validator",
    "schemas",
    "generate",
    "nrfcloud-application-protocols/schemas"
  ],
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/nrfcloud-application-protocols/"
    ]
  },
  "dependencies": {
    "@sinclair/typebox": "0.29.3",
    "ajv": "8.12.0",
    "jsonata": "2.0.3"
  }
}
